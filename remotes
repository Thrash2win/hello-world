<html>
<script>
function temp(){
  var i=0;
  for(;i<100;i++){
    if(i%3==0 && i%5==0)
      document.write(i+"fizzbuzz<br/>");
      else if(i%3==0)
              document.write(i+"fizz<br/>");
          else if(i%5==0)
                document.write(i+"buzz<br/>");
  }
};
</script>
<button onclick="temp()">Clicker</button>
</html>
